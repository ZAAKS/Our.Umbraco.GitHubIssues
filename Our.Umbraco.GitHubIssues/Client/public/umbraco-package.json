{
  "$schema": "https://json.schemastore.org/umbraco-package.json",
  "name": "Umbraco GitHubIssues Package",
  "version": "1.0.0",
  "allowTelemetry": true,

  "extensions": [
    {
      "type": "dashboard",
      "alias": "Our.Umbraco.GitHubIssues.Dashboard",
      "name": "Umbraco GitHub Issues Dashboard",
      "element": "/App_Plugins/Our.Umbraco.GitHubIssues/dashboard.js",
      "weight": 10,
      "meta": {
        "label": "GitHub Issues",
        "icon": "icon-lab",
        "elementName": "our-githubissues-dashboard"
      },
      "conditions": [
        {
          "alias": "Umb.Condition.SectionAlias",
          "match": "Umb.Section.Settings"
        }
      ]
    }
  ]
}